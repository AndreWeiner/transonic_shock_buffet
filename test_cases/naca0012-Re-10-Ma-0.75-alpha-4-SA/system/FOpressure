pressure
{
    // Mandatory entries (unmodifiable)
    type            pressure;
    libs            (fieldFunctionObjects);
    mode            totalCoeff;
    pInf            1.0e5;
    rhoInf          1.186;
    UInf            (256.99743845  17.97101155 0);

    executeControl  timeStep;
    executeInterval 25;
    writeControl    timeStep;
    writeInterval   25;
}

